<Window x:Class="DesktopContatcsApp.NewContactWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DesktopContatcsApp"
        mc:Ignorable="d"
        Title="NewContactWindow" Height="300" Width="300">
    <Grid>
        <StackPanel Margin="10">
            <TextBox Margin="0,0,0,5" x:Name="nameTextBox"/>
            <TextBox Margin="0,0,0,5" x:Name="emailTExtBox"/>
            <TextBox Margin="0,0,0,5" x:Name="phoneNumberTextBox" />
            <Button Content="Save" x:Name="saveButton" Click="saveButton_Click"/>
        </StackPanel>
    </Grid>
</Window>
